[Comment]

[UIPackage]



[Script]

str=Form1.InputBox1.Text
arr = split(str, vbcrlf)

For Each a In arr
    If Form1.OptionBox1.Value then
        Call cz(431, 339)//btsid
        LeftDoubleClick 1 //双击全选
    else
        Call cz(779, 340)//btsname
        KeyDown 17, 1 //Ctrl+A
        KeyPress 65, 1
        KeyUp 17, 1
    End If

    Plugin.Sys.SetCLB a
    KeyDown 17, 1 //Ctrl+V
    KeyPress 86, 1
    KeyUp 17, 1    
    Call cz(1160, 388) //查询
    Delay 300    
    Call cz(223,436 ) //勾选    
    Call cz(1192,387) //升级    
    Call cz(874,693) //确定    
    Call cz(717,552) //确定
Next

If Form1.OptionBox1.Value then
    Call cz(431, 339)//btsid
    LeftDoubleClick 1 //双击全选
else
    Call cz(779, 340)//btsname
    KeyDown 17, 1 //Ctrl+A
    KeyPress 65, 1
    KeyUp 17, 1
End If

KeyPress "Delete", 1

Sub cz(x, y)	
    MoveTo x,y
    LeftClick 1
    Delay 500	
End Sub

Event Form1.Button1.Click
    Form1.InputBox1.Text=""
End Event

Event Form1.Load
    Form1.OptionBox1.Value=True
End Event
